{
  "name": "pagination-window",
  "version": "1.0.0",
  "description": "Create a window of pages for populating a pagination control.",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Zeff Svoboda",
  "repository": "AlteredConstants/pagination-window",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
